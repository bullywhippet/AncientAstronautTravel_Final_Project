<h1>Categories</h1>
<div id="about" class="container-fluid">
  <div class="row">
    <div class="col-sm-8">
      <table>
        <tbody>
          <% @categories.each do |cat| %>
            <tr>
              <td><%= link_to cat.name, cat %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>